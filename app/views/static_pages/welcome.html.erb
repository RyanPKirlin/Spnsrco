<h1 class="greeting">Hey there!</h1>
<!-- add a class greeting to target with jquery -->

<p>
	<%= @math %>	
</p>

<p>Our last user is: 	<%= @last_user_name %> and the email is <%=@last_user_email %>. </p>

<%= form_for @new_user, :url => { :action => "create" } do |f| %>
  <%= f.text_field :name %>
  <%= f.text_field :email %>
  <%= f.submit "Create New Account", class: "btn btn-success" %>
<% end %>

<!-- You can look up other twitter bootstrap style, like the btn-success above -->